<?xml version="1.0" encoding='utf-8'?>
<openerp>
	<data>
	    <record id="stock_location_detail_form" model="ir.ui.view">
	    	<field name="name">stock.location.detail.form</field>
	    	<field name="model">stock.location.detail</field>
	    	<field name="arch" type="xml">
	    	    <form string="stock location detail">
	    	        <sheet>
	    	        	<group>
	    	        	    <div><h2><field name="product_id"></field></h2></div>
	    	        	    <group>
		                            <field name="location_id" />
		                            <field name="name"/>
		                    </group>
	    	        		<group>
	    	        		    <field name="code"></field>
	    	        		    <field name="parent_id"></field>
	    	        		    
	    	        		</group>
	    	        		<group>
	    	        			<field name="position_x"></field>
	    	        			<field name="position_y"></field>
	    	        			<field name="position_z"></field>
	    	        		</group>
	    	        		<group>
	    	        			<field name="position_str_x"></field>
	    	        			<field name="position_str_y"></field>
	    	        			<field name="position_str_z"></field>
	    	        		</group>
	    	        	</group>
	    	        </sheet>
	    	    </form>
	    	</field>
	    </record>
	    <record id="stock_location_detail_tree" model="ir.ui.view">
	    	<field name="name">stock.location.detail.tree</field>
	    	<field name="model">stock.location.detail</field>
	    	<field name="arch" type="xml">
	    	    <tree string="stock location detail">
	    	        <field name="name"></field>
	    	    	<field name="parent_id"></field>
	    	    	<field name="location_id"></field>
	    	    	<field name="code"></field>
	    	    	<field name="product_id"></field>
	    	    	<field name="position_x"></field>
	    	    	<field name="position_y"></field>
	    	    	<field name="position_z"></field>
	    	    	<field name="position_str_x"></field>
	    	    	<field name="position_str_y"></field>
	    	    	<field name="position_str_z"></field>
	    	    </tree>
	    	</field>
	    </record>
	    <record id="stock_product_detail_action" model="ir.actions.act_window">
	    	<field name="name">stock.location.detail</field>
	    	<field name="res_model">stock.location.detail</field>
	    	<field name="view_mode">tree,form</field>
	    	<field name="view_type">form</field>
	    </record>
	    <menuitem id="stock_product_detail_menu" name="stock product detail" parent="stock.menu_warehouse_config" action="stock_product_detail_action"/>
	</data>
</openerp>