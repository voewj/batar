<?xml version='1.0' encoding='utf-8'?>
<openerp>
	<data>
	    <record id="stock_move_detail_tree" model="ir.ui.view">
	    	<field name="name">stock.move.detail.tree</field>
	    	<field name="model">stock.move.detail</field>
	    	<field name="arch" type="xml">
	    	    <tree string="stock move detail" >
	    	        <field name="warehouse_id"></field>
	    	        <field name="default_code"></field>
	    	        <field name="product_id"></field>
	    	        <field name="attribute_value_ids" widget="many2many_tags"></field>
	    	        <field name="hand_qty"></field>
	    	        <field name="lock_qty"></field>
	    	        <field name="hand_weight"></field>
	    	        <field name="lock_weight"></field>
	    	    </tree>
	    	</field>
	    </record>
	    <record id="stock_move_detail_form" model="ir.ui.view">
	    	<field name="stock.move.detail.form"></field>
	    	<field name="model">stock.move.detail</field>
	    	<field name="arch" type="xml">
	    	    <form  string="stock move detail">
	    	        <sheet>
	    	            <group>
	    	            	<group>
	    	            	    <field name="warehouse_id" />
	    	            	    <field name="product_id" />
	    	            	    <field name="default_code" readonly='1' />
	    	            	    <field name="attribute_value_ids" widget="many2many_tags" readonly='1'/>
	    	            	</group>
	    	            	<group>
	    	            		<field name="hand_qty" readonly="1"></field>
	    	            		<field name="lock_qty" readonly="1"></field>
	    	            		<field name="hand_weight" readonly="1"></field>
	    	            		<field name="lock_weight" readonly="1"></field>
	    	            	</group>
	    	            </group>
	    	        </sheet>
	    	    </form>
	    	</field>
	    </record>
	</data>
</openerp>